{
  "name": "pta/composer-installer",
  "description": "Module Installer for Core",
  "type": "composer-installer",
  "license": "MIT",
  "authors": [
    {
      "name": "The Package Team Alliance",
      "email": "",
      "homepage": "https://bitbucket.org/packageteamalliance/composer-installer"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~5.0",
    "composer-plugin-api": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Pta\\Installer\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "class": "Pta\\Installer\\ModuleInstaller"
  },
  "minimum-stability": "dev"
}
